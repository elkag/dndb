<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dndm.app.setup.wizard.items.item.ItemController">
   <VBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
      <HBox prefHeight="108.0" prefWidth="386.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <ImageView fx:id="avatar" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
         <VBox alignment="CENTER">
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <HBox spacing="3.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <Label text="Name:" />
               <Label fx:id="name" text="Label" />
            </HBox>
            <HBox layoutX="20.0" layoutY="20.0" spacing="3.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <Label text="Type:" />
               <Label fx:id="type" text="Label" />
            </HBox>
         </VBox>
         <VBox layoutX="120.0" layoutY="20.0">
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <HBox spacing="3.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <Label text="Max Items:" />
               <Label fx:id="maxItems" text="Label" />
            </HBox>
            <HBox layoutX="20.0" layoutY="20.0" spacing="3.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <Label text="Max Gold:" />
               <Label fx:id="maxGold" text="Label" />
            </HBox>
            <HBox layoutX="20.0" layoutY="57.0" spacing="3.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <Label text="Pricemod:" />
               <Label fx:id="pricemod" text="Label" />
            </HBox>
         </VBox>
         <HBox alignment="TOP_RIGHT" HBox.hgrow="ALWAYS">
            <Button mnemonicParsing="false" onAction="#deleteMerchant" text="X" textFill="#fc0303">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </HBox>
      </HBox>
      <Label fx:id="settlement" text="Label">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
   </VBox>

</AnchorPane>
